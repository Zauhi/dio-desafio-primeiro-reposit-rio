algoritmo "semnome"
// Função :
// Autor :
// Data : 02/03/22
// Seção de Declarações 
var
   X, Y: Inteiro
   Procedimento Soma(A, B: Inteiro)
   inicio
         EscrevaL ("Recebi o valor ",A)
         EscrevaL ("Recebi o valor ",B)
         EscrevaL ("A soma entre os dois é ", A+B)
   FimProcedimento
inicio
// Seção de Comandos 
   X <- 5
   Y <- 3
   Soma(X, Y)
fimalgoritmo
